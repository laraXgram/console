{
  "name": "laraxgram/console",
  "description": "The LaraGram Console package.",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/laraxgram/core/issues",
    "source": "https://github.com/laraxgram/core"
  },
  "authors": [
    {
      "name": "LaraGram",
      "email": "laraxgram@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-mbstring": "*",
    "laraxgram/collections": "^3.0",
    "laraxgram/contracts": "^3.0",
    "laraxgram/macroable": "^3.0",
    "laraxgram/support": "^3.0"
  },
  "autoload": {
    "files": [
      "Prompts/helpers.php"
    ],
    "psr-4": {
      "LaraGram\\Console\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "suggest": {
    "ext-pcntl": "Required to use signal trapping.",
    "laraxgram/bus": "Required to use the scheduled job dispatcher (^3.0).",
    "laraxgram/container": "Required to use the scheduler (^3.0).",
    "laraxgram/filesystem": "Required to use the generator command (^3.0).",
    "laraxgram/queue": "Required to use closures for scheduled jobs (^3.0)."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}